<div class="article-index-content pure-g">
	<div class="articles-content pure-u-2-3">
		<% @articles.zip(@graphics).each do |article, graphic| %>
			<div class="article-box">
				<p><%= article.headline %></p>
				<p><%= link_to image_tag(graphic.image, alt: "Article Graphic", width: '400'), "#" %></p>
			</div>
		<% end %>
	</div>
	<div class="sidebar-content pure-u-1-3">
		<p>big whoop</p>
	</div>
</div>

<%= will_paginate @articles %>
